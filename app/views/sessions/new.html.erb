<% provide(:title, 'Log in') %>
<%= form_for :session, url: login_path do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :remember_me %>
  <%= f.check_box :rememeber_me %>
  <%= f.submit 'Log in' %>
<% end %>
